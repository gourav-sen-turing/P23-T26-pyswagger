{
   "swagger":"2.0",
   "host":"http://test.com",
   "basePath":"/v1",
   "paths":{
      "/t":{
         "get":{
            "parameters":[
               {
                  "name":"p1",
                  "in":"header",
                  "type":"array",
                  "items":{
                     "type":"integer",
                     "format":"int64"
                  },
                  "collectionFormat":"csv"
               },
               {
                  "name":"p2",
                  "in":"header",
                  "type":"integer",
                  "format":"int32",
                  "maximum":100,
                  "exclusiveMaximum":true,
                  "minimum":0,
                  "exclusiveMinimum":false
               },
               {
                  "name":"p3",
                  "in":"header",
                  "type":"array",
                  "items":{
                     "type":"array",
                     "items":{
                        "type":"array",
                        "items":{
                           "type":"integer",
                           "format":"int64"
                        },
                        "collectionFormat":"pipes"
                     },
                     "collectionFormat":"csv"
                  },
                  "collectionFormat":"ssv"
               }
            ]
         },
         "put":{
            "parameters":[
               {
                  "name":"p1",
                  "in":"query",
                  "type":"string"
               },
               {
                  "name":"p2",
                  "in":"query",
                  "type":"string"
               },
               {
                  "name":"p3",
                  "in":"query",
                  "type":"string"
               }
            ]
         }
      }
   },
   "definitions":{
      "Category":{
         "properties":{
            "id":{
               "type":"integer",
               "format":"int64",
               "xml":{
                  "name":"id"
               }
            },
            "name":{
               "type":"string",
               "xml":{
                  "name":"name"
               }
            }
         },
         "xml":{
            "name":"Category"
         }
      },
      "Pet":{
         "required":[
            "name",
            "photoUrls"
         ],
         "properties":{
            "id":{
               "type":"integer",
               "format":"int64",
               "xml":{
                  "name":"id"
               }
            },
            "category":{
               "xml":{
                  "name":"category"
               },
               "$ref":"Category"
            },
            "name":{
               "type":"string",
               "example":"doggie",
               "xml":{
                  "name":"name"
               }
            },
            "photoUrls":{
               "type":"array",
               "xml":{
                  "name":"photoUrl",
                  "wrapped":true
               },
               "items":{
                  "type":"string"
               }
            },
            "tags":{
               "type":"array",
               "xml":{
                  "name":"tag",
                  "wrapped":true
               },
               "items":{
                  "$ref":"Tag"
               }
            },
            "status":{
               "type":"string",
               "xml":{
                  "name":"status"
               },
               "description":"pet status in the store"
            }
         },
         "xml":{
            "name":"Pet"
         }
      },
      "Tag":{
         "properties":{
            "id":{
               "type":"integer",
               "format":"int64",
               "xml":{
                  "name":"id"
               }
            },
            "name":{
               "type":"string",
               "xml":{
                  "name":"name"
               }
            }
         },
         "xml":{
            "name":"Tag"
         }
      },
      "User":{
         "properties":{
            "id":{
               "type":"integer",
               "format":"int64"
            },
            "location":{
               "type":"string"
            }
         }
      },
      "Skill":{
         "properties":{
            "skill_id":{
               "type":"integer",
               "format":"int64"
            },
            "skill_name":{
               "type":"string"
            }
         }
      },
      "Employee":{
         "allOf":[
            {
               "$ref":"User"
            },
            {
               "$ref":"Skill"
            }
         ]
      },
      "Boss":{
         "properties":{
            "boss_name":{
               "type":"string"
            }
         },
         "allOf":[
            {
               "$ref":"User"
            },
            {
               "$ref":"Skill"
            }
         ]
      },
      "int":{
         "type":"integer",
         "format":"int32",
         "allOf":[
            {
               "maximum":200,
               "exclusiveMaximum":true
            },
            {
               "minimum":100,
               "exclusiveMinimum":false
            }
         ]
      },
      "num_multipleOf":{
         "type":"number",
         "format":"float",
         "allOf":[
            {
               "multipleOf":5
            }
         ]
      },
      "byte":{
         "type":"string",
         "format":"byte"
      },
      "date":{
         "type":"string",
         "format":"date"
      },
      "date-time":{
         "type":"string",
         "format":"date-time"
      },
      "str_enum":{
         "type":"string",
         "enum":[
            "red",
            "blue",
            "green"
         ]
      },
      "model_bool":{
         "properties":{
            "bool_val":{
               "type":"boolean"
            }
         }
      },
      "array_int":{
          "type":"array",
          "items":{
              "type":"integer",
              "format":"int32"
          },
          "maxItems":5,
          "minItems":2
      }
   }
}
